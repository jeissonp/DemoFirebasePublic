  pod 'Firebase/Auth'
  pod 'SCLAlertView'

  target 'DemoFirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DemoFirebaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
